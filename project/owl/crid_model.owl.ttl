@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix crid_model: <https://w3id.org/dalito/crid-model/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

crid_model:CRIDRecordCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CRIDRecordCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:entries ],
        [ a owl:Restriction ;
            owl:allValuesFrom crid_model:CRIDRecord ;
            owl:onProperty crid_model:entries ] ;
    skos:definition "A holder for CRIDRecord objects" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:CRIDRecord a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CRIDRecord" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:birth_date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty crid_model:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty crid_model:birth_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty crid_model:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom crid_model:PersonStatus ;
            owl:onProperty crid_model:vital_status ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:age_in_years ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:age_in_years ],
        crid_model:NamedThing ;
    skos:definition "Represents a CRIDRecord" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty crid_model:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty crid_model:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty crid_model:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty crid_model:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty crid_model:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty crid_model:description ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

PATO:0001421 a owl:Class,
        crid_model:PersonStatus ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf crid_model:PersonStatus .

PATO:0001422 a owl:Class,
        crid_model:PersonStatus ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf crid_model:PersonStatus .

<https://w3id.org/dalito/crid-model/PersonStatus#UNKNOWN> a owl:Class,
        crid_model:PersonStatus ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf crid_model:PersonStatus .

crid_model:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:birth_date a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:primary_email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range crid_model:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/dalito/crid-model> .

crid_model:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/dalito/crid-model/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/dalito/crid-model/PersonStatus#UNKNOWN> .

<https://w3id.org/dalito/crid-model> a owl:Ontology ;
    rdfs:label "crid-model" ;
    dcterms:license "MIT" ;
    dcterms:title "crid-model" ;
    rdfs:seeAlso <https://dalito.github.io/crid-model> ;
    skos:definition "LinkML model for Catalyst Resource Idenifier (CRID)" .

